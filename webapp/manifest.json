{
	"_version": "1.7.0",
	"sap.app": {
		"id": "transferencia.depositoZ_TRANSF_DEP",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"Z_TRANS_DEP_SRV": {
				"uri": "/sap/opu/odata/sap/Z_TRANS_DEP_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "transferencia.depositoZ_TRANSF_DEP.view.FiltroReserva",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "transferencia.depositoZ_TRANSF_DEP.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneTime",
					"defaultCountMode": "Request"
				},
				"dataSource": "Z_TRANS_DEP_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"viewPath": "transferencia.depositoZ_TRANSF_DEP.view",
				"controlId": "fcl",
				"transition": "slide"
			},
			"routes": [
				{
					"pattern": ":layout:",
					"name": "FiltroReserva",
					"target": [
						"FiltroReserva"
					],
					"layout": "OneColumn"
				},
				{
					"pattern": "reserva/{aufnr}/{rsnum}/{rspos}/:layout:",
					"name": "ProcessarReserva",
					"target": [
						"FiltroReserva",
						"ProcessarReserva"
					],
					"layout": "TwoColumnsMidExpanded"
				}
			],
			"targets": {
				"FiltroReserva": {
					"viewName": "FiltroReserva",
					"controlAggregation": "beginColumnPages"
				},
				"ProcessarReserva": {
					"viewName": "ProcessarReserva",
					"controlAggregation": "midColumnPages"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/z_transf_dep/webapp",
		"_version": "1.1.0"
	}
}